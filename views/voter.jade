extends layout

block content
    div(class="content-wrapper")
        div(class="container")
            section(class="content-header")
                h1.
            section(class="content", ng-controller='voterCTRL')
                div(class="row")
                    div(class="col-md-12")
                        div(class="box box-primary")
                            div(class="box-header with-border")
                                h3(class="box-title").
                                    Registration
                            div(class="box-body")
                                div(class="form-group")
                                   label.
                                       Email
                                   input(type="email", class="form-control", placeholder="Enter email", ng-model="voter.email")
                            div(class="box-footer")
                                button(class="btn btn-primary", ng-click="save()").
                                   Save
    script(src="/javascripts/jQuery-2.1.4.min.js")
    script(src="/bootstrap/js/bootstrap.min.js")
    script(src="/datatables/jquery.dataTables.js")
    script(src="/datatables/dataTables.bootstrap.js")
    script(src="/javascripts/test.js")
    script(src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.0/angular.min.js")
    script(src="/javascripts/controller.js")
    script(src="/javascripts/app.min.js")
    script(src="/javascripts/demo.js")